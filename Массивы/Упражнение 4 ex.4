#include <stdio.h>
int main()
{
    int M [] = {1, 2, 3, 4, 5, 6}; // определяем массив
 
    int B = sizeof(M) / sizeof(M [0]);  
    int C = B / 2;    
    for(int i = 0; i < C; i++)
    {
        int E = M [i];
        M [i] = M [B-i-1];
        M [B-i-1] = E;
    }
   
    for(int i = 0; i < B; i++)
    {
        printf("%d", M[i]);
    }
    return 0;
}
